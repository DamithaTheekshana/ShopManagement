<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.HomePageController">
    <children>
        <Label layoutX="25.0" layoutY="28.0" text="Thoga Kade Management System">
            <font>
                <Font name="Arial Black" size="30.0" />
            </font>
        </Label>
        <Button fx:id="btnCustomerManagement" layoutX="174.0" layoutY="117.0" mnemonicParsing="false" onAction="#CustomerManagementOnAction" text="Customer Management">
            <font>
                <Font name="Arial Rounded MT Bold" size="20.0" />
            </font>
        </Button>
        <Button fx:id="btnItemManagement" layoutX="193.0" layoutY="182.0" mnemonicParsing="false" onAction="#ItemManagementOnAction" prefHeight="37.0" prefWidth="215.0" text="Item Management">
            <font>
                <Font name="Arial Rounded MT Bold" size="20.0" />
            </font>
        </Button>
        <Button fx:id="btnOrderManagement" layoutX="193.0" layoutY="248.0" mnemonicParsing="false" onAction="#OrderManagementOnAction" text="Order Management">
            <font>
                <Font name="Arial Rounded MT Bold" size="20.0" />
            </font>
        </Button>
        <Button fx:id="btnOrderDetailManagement" layoutX="165.0" layoutY="315.0" mnemonicParsing="false" onAction="#OrderDetailManagementOnAction" text="OrderDetail Management">
            <font>
                <Font name="Arial Rounded MT Bold" size="20.0" />
            </font>
        </Button>
    </children>
</AnchorPane>

